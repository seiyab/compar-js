{
  "name": "@seiyab/compar",
  "version": "0.0.4",
  "description": "Comparators and Comparator Combinators for JavaScript",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "lint": "eslint ./src --ext .ts",
    "format": "prettier -w \"src/**/*.ts\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
